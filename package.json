{
  "name": "@fingerprintjs/fingerprintjs-pro-vue-v3",
  "version": "1.4.0",
  "description": "Fingerprint Pro Plugin for Vue v3",
  "author": "FingerprintJS, Inc (https://fingerprintjs.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fingerprintjs/fingerprintjs-pro-vue.git"
  },
  "exports": {
    ".": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.esm.js",
      "require": "./dist/plugin.cjs.js",
      "node": "./dist/plugin.esm.js"
    }
  },
  "main": "dist/plugin.esm.js",
  "module": "dist/plugin.mjs",
  "types": "dist/plugin.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "keywords": [
    "fraud",
    "fraud detection",
    "fraud prevention",
    "browser",
    "identification",
    "fingerprint",
    "fingerprinting",
    "browser fingerprint",
    "device fingerprint",
    "privacy"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "private": false,
  "scripts": {
    "prepare": "husky install",
    "cm": "cz",
    "build": "rimraf dist && rollup -c rollup.config.js --bundleConfigAsCjs",
    "lint": "eslint --ext .js,.ts,.tsx --ignore-path .gitignore --max-warnings 0 .",
    "lint:fix": "pnpm lint --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:dts": "tsc --noEmit --isolatedModules --lib es2022,dom dist/plugin.d.ts",
    "docs": "typedoc src/index.ts --out docs",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@fingerprintjs/commit-lint-dx-team": "^0.0.2",
    "@fingerprintjs/eslint-config-dx-team": "^0.1.0",
    "@fingerprintjs/prettier-config-dx-team": "^0.1.0",
    "@fingerprintjs/tsconfig-dx-team": "^0.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@semantic-release/exec": "6.0.3",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vue/test-utils": "^2.0.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "husky": "^8.0.1",
    "jest": "^29.7.0",
    "lint-staged": "^13.0.3",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-license": "^3.3.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.25.12",
    "typescript": "^5.4.2",
    "vite": "^4.5.2",
    "vite-plugin-dts": "^3.7.3",
    "vue": "^3.1",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^2.0.6"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs-pro-spa": "^1.2.0",
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "vue": "^3.1"
  },
  "lint-staged": {
    "*.ts": "pnpm lint:fix",
    "*.tsx": "pnpm lint:fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
