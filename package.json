{
  "name": "@fingerprintjs/fingerprintjs-pro-vue",
  "version": "0.0.1",
  "description": "FingerprintJS Pro Plugin for Vue",
  "author": "FingerprintJS, Inc (https://fingerprintjs.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fingerprintjs/fingerprintjs-pro-vue.git"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c rollup.config.js",
    "build:spa": "cd examples/spa && yarn build && cd ../..",
    "watch": "rollup -c rollup.config.js -w",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --max-warnings 0 .",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:dts": "tsc --noEmit dist/plugin.d.ts"
  },
  "main": "dist/plugin.cjs.js",
  "module": "dist/plugin.esm.js",
  "types": "dist/plugin.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/test-utils": "^2.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.0.1",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "rollup": "^2.70.1",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-license": "^2.7.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5",
    "vite": "^2.9.9",
    "vue": "^3.2.36",
    "vue-eslint-parser": "^9.0.2",
    "vue-tsc": "^0.34.7"
  },
  "peerDependencies": {
    "vue": "^3.2.36"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs-pro-spa": "^0.3.1"
  }
}
