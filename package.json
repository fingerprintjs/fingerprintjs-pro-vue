{
  "name": "@fingerprintjs/fingerprintjs-pro-vue-v3",
  "version": "1.4.0",
  "description": "Fingerprint Pro Plugin for Vue v3",
  "author": "FingerprintJS, Inc (https://fingerprintjs.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fingerprintjs/fingerprintjs-pro-vue.git"
  },
  "main": "dist/plugin.cjs.js",
  "module": "dist/plugin.esm.js",
  "types": "dist/plugin.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "keywords": [
    "fraud",
    "fraud detection",
    "fraud prevention",
    "browser",
    "identification",
    "fingerprint",
    "fingerprinting",
    "browser fingerprint",
    "device fingerprint",
    "privacy"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "private": false,
  "scripts": {
    "prepare": "husky install",
    "cm": "cz",
    "build": "rimraf dist && rollup -c rollup.config.js",
    "lint": "eslint --ext .js,.ts,.tsx --ignore-path .gitignore --max-warnings 0 .",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:dts": "tsc --noEmit --isolatedModules dist/fp-pro-react.d.ts",
    "docs": "typedoc src/index.ts --out docs",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@semantic-release/exec": "6.0.3",
    "@theunderscorer/nx-semantic-release": "1.11.0",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vue/test-utils": "^2.0.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.0.1",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "rimraf": "^5.0.5",
    "rollup": "^2.70.1",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-license": "^2.7.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript-paths": "^1.3.1",
    "ts-jest": "^27.1.3",
    "typedoc": "^0.22.17",
    "typescript": "^4.5.5",
    "vite": "^4.4.9",
    "vue": "^3.1",
    "vue-eslint-parser": "^9.0.2",
    "vue-tsc": "^1.2.0"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs-pro-spa": "^1.2.0"
  },
  "peerDependencies": {
    "vue": "^3.1"
  }
}
