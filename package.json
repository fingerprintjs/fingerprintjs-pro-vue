{
  "name": "@fingerprintjs/fingerprintjs-pro-vue",
  "description": "FingerprintJS Pro Plugin for Vue monorepo",
  "author": "FingerprintJS, Inc (https://fingerprintjs.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fingerprintjs/fingerprintjs-pro-vue.git"
  },
  "private": true,
  "workspaces": {
    "packages": [
      "packages/**",
      "examples/**"
    ],
    "nohoist": [
      "**/vue",
      "vue",
      "@nuxt/**",
      "**/@nuxt/**",
      "nuxt/**",
      "**/nuxt/**"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "cm": "cz",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint:fix --all",
    "test": "nx run-many --target=test --all",
    "test:dts": "nx run-many --target=test:dts --all",
    "build": "nx run-many --target=build --all",
    "docs": "nx run-many --target=docs --all",
    "semantic-release": "nx run-many --target=semantic-release --all --maxParallel=1",
    "nx": "nx"
  },
  "resolutions": {
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@nrwl/workspace": "^14.1.9",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@semantic-release/exec": "6.0.3",
    "@theunderscorer/nx-semantic-release": "1.11.0",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.0.1",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "nx": "14.1.9",
    "prettier": "^2.5.1",
    "rollup": "^2.70.1",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-license": "^2.7.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript-paths": "^1.3.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-jest": "^27.1.3",
    "typedoc": "^0.22.17",
    "typescript": "^4.5.5",
    "vue-eslint-parser": "^9.0.2",
    "vue-tsc": "^1.2.0",
    "vite": "^4.4.9"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs-pro-spa": "^1.2.0"
  },
  "nxrelease": {
    "repositoryUrl": "https://github.com/fingerprintjs/fingerprintjs-pro-vue",
    "branches": [
      "main",
      {
        "name": "test",
        "prerelease": true
      }
    ],
    "github": true,
    "preset": "conventionalCommits",
    "presetConfig": {
      "types": [
        {
          "type": "feat",
          "section": "Features"
        },
        {
          "type": "feature",
          "section": "Features"
        },
        {
          "type": "fix",
          "section": "Bug Fixes"
        },
        {
          "type": "perf",
          "section": "Performance Improvements"
        },
        {
          "type": "revert",
          "section": "Reverts"
        },
        {
          "type": "docs",
          "scope": "README",
          "section": "Documentation"
        },
        {
          "type": "build",
          "scope": "deps",
          "section": "Build System"
        },
        {
          "type": "docs",
          "section": "Documentation",
          "hidden": true
        },
        {
          "type": "style",
          "section": "Styles",
          "hidden": true
        },
        {
          "type": "chore",
          "section": "Miscellaneous Chores",
          "hidden": true
        },
        {
          "type": "refactor",
          "section": "Code Refactoring",
          "hidden": true
        },
        {
          "type": "test",
          "section": "Tests",
          "hidden": true
        },
        {
          "type": "build",
          "section": "Build System",
          "hidden": true
        },
        {
          "type": "ci",
          "section": "Continuous Integration",
          "hidden": true
        }
      ]
    },
    "releaseRules": [
      {
        "type": "build",
        "scope": "deps",
        "release": "patch"
      },
      {
        "type": "docs",
        "scope": "README",
        "release": "patch"
      }
    ]
  }
}
