{
  "projectType": "library",
  "tags": [],
  "implicitDependencies": ["shared"],
  "targets": {
    "semantic-release": {
      "executor": "@theunderscorer/nx-semantic-release:semantic-release",
      "options": {
        "outputPath": "packages/fingerprintjs-pro-vue-v3",
        "tagFormat": "fingerprintjs-pro-vue3-v${version}",
        "packageJsonDir": "packages/fingerprintjs-pro-vue-v3",
        "plugins": [
          [
            "@semantic-release/exec",
            {
              "prepareCmd": "npx nx run fingerprintjs-pro-vue-v3:build --skip-nx-cache"
            }
          ]
        ]
      },
      "configurations": {
        "dry": {
          "dryRun": true
        }
      }
    }
  }
}
