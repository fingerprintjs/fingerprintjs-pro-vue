{
  "name": "@fingerprintjs/fingerprintjs-pro-vue-v2",
  "version": "1.2.0",
  "description": "FingerprintJS Pro Plugin for Vue v2",
  "author": "FingerprintJS, Inc (https://fingerprintjs.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fingerprintjs/fingerprintjs-pro-vue.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "private": false,
  "keywords": [
    "fraud",
    "fraud detection",
    "fraud prevention",
    "browser",
    "identification",
    "fingerprint",
    "fingerprinting",
    "browser fingerprint",
    "device fingerprint",
    "privacy"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c rollup.config.js",
    "watch": "rollup -c rollup.config.js -w",
    "test": "jest",
    "test:dts": "tsc --noEmit --isolatedModules --skipLibCheck dist/plugin.d.ts",
    "lint": "eslint --ext .js,.ts,.vue --max-warnings 0 .",
    "lint:fix": "yarn lint --fix",
    "docs": "typedoc src/index.ts --out ../../docs/vue-2"
  },
  "main": "dist/plugin.cjs.js",
  "module": "dist/plugin.esm.js",
  "types": "dist/plugin.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "vue": "^2.6"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.3.0",
    "vue": "^2.6",
    "vue-template-compiler": "^2.6.14"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs-pro-spa": "^1.1.3"
  }
}
